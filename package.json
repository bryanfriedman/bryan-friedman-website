{
  "name": "bryan-friedman",
  "title": "Bryan Friedman",
  "version": "1.0",
  "homepage": "https://www.bryanfriedman.com",
  "author": "Bryan Friedman",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "kill": "kill $(ps -ef | grep '[n]pm exec @11ty/eleventy --serve' | awk '{print $2}')",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npx rimraf dist public",
    "search": "npx pagefind --site dist",
    "eleventy": "npm run clean && npx @11ty/eleventy && npm run search",
    "dev": "npm run eleventy && open=true npx @11ty/eleventy --serve",
    "build": "npm run eleventy && cp -r ./dist/ ./public"
  },
  "devDependencies": {
    "@bryanfriedman/eleventy-plugin-html-relative-datacascade": "0.1.5",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.1",
    "eleventy-plugin-purgecss": "^0.5.0",
    "esbuild": "^0.27.2",
    "html-minifier-terser": "^7.2.0",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "open": "10.2.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "sass": "1.60.0",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-bundle": "^3.0.7",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "bootstrap": "^5.3.3",
    "lite-youtube-embed": "^0.3.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-tweet": "^3.3.0"
  }
}
